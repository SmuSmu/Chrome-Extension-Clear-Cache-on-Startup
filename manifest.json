{
  "name": "Clear Cache",
  "version": "1.0",
  "description": "Clear Cache on Startup",
  "permissions": ["browsingData"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
    },
  "icons": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "48": "img/icon_48.png",
      "128": "img/icon_128.png"
    },
  "manifest_version": 2
}
